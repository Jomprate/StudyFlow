{
  "ConnectionStrings": {
    "StudyFlowDB": "Server=tcp:studyflow-db.database.windows.net,1433;Initial Catalog=StudyFlowDB;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;Authentication=Active Directory Default;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AzureKeyVault": {
    "KeyVaultUri": "https://studyflowkey.vault.azure.net/"
  },
  "AzureContainerName": {
    "ImageContainer": "image"
  },
  "JwtConfig": {
    "Issuer": "https://studyflow.azurewebsites.net",
    "Audience": "https://studyflowbackend.azurewebsites.net",
    "ExpiryDurationLogin": 60,
    "ExpiryDurationRecovery": 30,
    "ExpiryDurationShare": 1440
  },
  "AllowedHosts": "*"
}